# Database
DATABASE_URL=postgresql://username:password@localhost:5432/titletesterpro

# Google OAuth (for YouTube API access)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
# OAuth Redirect URI (set this in production deployment)
OAUTH_REDIRECT_URI=https://titletesterpro.com/api/auth/callback/google

# Encryption key for OAuth tokens
ENCRYPTION_KEY=your_32_character_encryption_key

# Session configuration
NEXTAUTH_SECRET=your_session_secret
NEXTAUTH_URL=http://localhost:5000

# YouTube API
YOUTUBE_API_KEY=your_youtube_api_key

# Scheduler (if using external job queue)
AGENDA_MONGO_URL=mongodb://localhost:27017/titletesterpro
